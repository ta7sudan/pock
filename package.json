    "fastify-cors": "^0.1.0",
    "fastify-http-proxy": "^0.7.0",
    "figlet": "^1.2.0",
    "figures": "^2.0.0",
    "got": "^9.2.2",
    "js-yaml": "^3.12.0",
    "semver": "^5.5.1",
    "yargonaut": "^1.1.4",
    "yargs": "^12.0.2"
  },
  "devDependencies": {
    "eslint": "^5.6.0",
    "eslint-plugin-prettier": "^2.6.2",
    "husky": "^1.0.0-rc.14",
    "jsinspect": "^0.12.7",
    "lint-staged": "^7.2.2",
    "prettier": "^1.14.2"
  }
}
