{
  "name": "pock",
  "version": "0.1.0",
  "description": "A mock server",
  "main": "src/index.js",
  "bin": {
    "pock": "./bin/index.js"
  },
  "man": "./man/doc.1",
  "engines": {
    "node": ">=10.0.0"
  },
  "scripts": {
    "lint": "eslint src bin test",
    "format": "prettier --config .prettierrc.js --write src/**/*.js bin/**/*.js",
    "inspect": "jsinspect src bin",
    "test": "echo todo",
    "prepare": "npm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ta7sudan/pock.git"
  },
  "keywords": [
    "mock",
    "server",
    "cli"
  ],
  "author": "ta7sudan",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/ta7sudan/pock/issues"
  },
  "homepage": "https://github.com/ta7sudan/pock#readme",
  "files": [
    "/bin",
    "/src",
    "/man",
    "/docs",
    "/examples"
  ],
  "lint-staged": {
    "*.js": [
      "eslint"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm test"
    }
  },
  "dependencies": {
    "chalk": "^2.4.1",
    "chokidar": "^2.0.4",
    "fast-glob": "^2.2.3",
